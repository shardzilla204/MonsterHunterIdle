; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Monster Hunter Idle"
config/version="0.2.1"
run/main_scene="res://Scenes/PackedScenes/Interface/GameInterface.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="Godot/app_userdata/MonsterHunterIdle"
config/features=PackedStringArray("4.4", "C#", "GL Compatibility")
config/icon="res://Assets/Images/MonsterHunterLogo.png"

[autoload]

MonsterHunterIdle="*res://Scenes/MonsterHunterIdle.tscn"
OfflineProgress="*res://Scripts/OfflineProgress.cs"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/vsync/vsync_mode=0

[dotnet]

project/assembly_name="MonsterHunterIdle"

[editor]

naming/scene_name_casing=1
naming/script_name_casing=1

[file_customization]

folder_colors={
"res://Assets/": "orange",
"res://JSON/": "yellow",
"res://Scenes/": "blue",
"res://Scripts/": "purple"
}

[input]

Attack={
"deadzone": 0.2,
"events": [Object(InputEventMouseButton,"resource_local_to_scene":false,"resource_name":"","device":-1,"window_id":0,"alt_pressed":false,"shift_pressed":false,"ctrl_pressed":false,"meta_pressed":false,"button_mask":1,"position":Vector2(89, 19),"global_position":Vector2(98, 67),"factor":1.0,"button_index":1,"canceled":false,"pressed":true,"double_click":true,"script":null)
]
}

[rendering]

textures/canvas_textures/default_texture_filter=3
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
